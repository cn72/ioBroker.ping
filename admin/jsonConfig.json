{
  "i18n": true,
  "type": "tabs",
  "items": {
    "_main": {
      "type": "panel",
      "label": "Main settings",
      "items": {
        "interval": {
          "type": "number",
          "min": 5000,
          "label": "Interval",
          "help": "ms",
          "xs": 12,
          "sm": 12,
          "md": 2
        },
        "intervalByUnreach": {
          "type": "number",
          "min": 5000,
          "label": "Interval if unreachable",
          "help": "ms",
          "xs": 12,
          "sm": 12,
          "md": 2
        },
        "numberOfRetries": {
          "newLine": true,
          "type": "number",
          "min": 0,
          "label": "Number of retries",
          "xs": 12,
          "sm": 12,
          "md": 2
        },
        "noHostname": {
          "newLine": true,
          "type": "checkbox",
          "label": "No host name in state name",
          "xs": 12,
          "sm": 12,
          "md": 4
        },
        "setcap": {
            "newLine": true,
            "type": "checkbox",
            "label": "Allow with setcap the required rights for ping",
            "xs": 12,
            "sm": 12,
            "md": 4,
            "help": "This allows to run ping without root rights"
        }
      }
    },
    "_devices": {
      "type": "panel",
      "label": "Devices",
      "items": {
        "devices": {
          "type": "table",
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "showSecondAddAt": 10,
          "export": true,
          "import": true,
          "items": [
            {
              "type": "checkbox",
              "attr": "enabled",
              "title": "Enabled",
              "width": 70
            },
            {
              "type": "text",
              "attr": "name",
              "title": "Name"
            },
            {
              "type": "checkbox",
              "attr": "use_name",
              "title": "Use name",
              "width": 70
            },
            {
              "type": "text",
              "attr": "ip",
              "title": "IP Address"
            },
            {
              "type": "checkbox",
              "attr": "extended_info",
              "title": "Extended Info",
              "width": 70
            }
          ]
        }
      }
    },
    "_browse": {
      "type": "panel",
      "label": "Browse",
      "items": {
        "_browse1": {
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 6,
          "lg": 6,
          "i18n": true,
          "url": "custom/customComponents.js",
          "name": "ConfigCustomPingSet/Components/PingBrowseComponent",
          "type": "custom"
        }
      }
    }
  }
}
